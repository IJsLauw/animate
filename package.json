{
  "name": "pixi-animate",
  "description": "PIXI plugin for the PixiAnimate Extension",
  "version": "1.3.5",
  "main": "lib/pixi-animate.js",
  "module": "lib/pixi-animate.es.js",
  "bundle": "dist/pixi-animate.js",
  "typings": "lib/pixi-animate.d.ts",
  "author": "Matt Karl <matt.karl@jibo.com>",
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.1",
    "@types/pixi.js": "^4.0.0",
    "browserify-derequire": "^0.9.4",
    "electron": "^4.0.6",
    "electron-prebuilt": "^1.2.7",
    "eslint": "^4.18.2",
    "floss": "^1.2.0",
    "gh-pages": "^2.0.0",
    "http-server": "*",
    "js-md5": "^0.4.1",
    "pixi.js": "^4.2.2",
    "rimraf": "^2.5.4",
    "rollup": "^1.32.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-jscc": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-ts": "^1.2.14",
    "semver": "^5.3.0",
    "terser": "^3.16.1",
    "tslib": "^1.9.3",
    "typedoc": "^0.15.0",
    "typescript": "^3.3.0"
  },
  "peerDependencies": {
    "pixi.js": ">=4.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/pixi-animate"
  },
  "bugs": {
    "url": "https://github.com/pixijs/pixi-animate/issues"
  },
  "homepage": "https://github.com/pixijs/pixi-animate",
  "scripts": {
    "clean": "rimraf docs/docs docs/dist .publish",
    "lint": "eslint src",
    "prebuild": "npm run lint",
    "build": "tsc && rollup -c && npm run build:min && npm run build:dts",
    "build:min": "terser dist/pixi-animate.js -c -m -o dist/pixi-animate.min.js --source-map \"content='dist/pixi-animate.js.map',includeSources=true,filename='dist/pixi-animate.min.js.map',url='pixi-animate.min.js.map'\" --comments \"/pixi-animate.js - /\"",
    "build:dts": "node build/buildAmbientTypes.js",
    "test": "floss --path tests/index.js",
    "coverage": "npm test -- -c dist/pixi-animate.js -s -h",
    "test:debug": "npm test -- --debug",
    "docs": "typedoc",
    "predocs-live": "npm run docs",
    "docs-live": "gh-pages -d docs",
    "postversion": "npm run build && npm test",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "postpublish": "git push && git push --tags && npm run docs-live"
  },
  "files": [
    "dist/",
    "lib/",
    "LICENSE",
    "package.json",
    "README.md",
    "index.d.ts",
    "ambient.d.ts"
  ],
  "keywords": [
    "webgl",
    "pixi",
    "pixi.js",
    "Flash",
    "Animate CC",
    "animation"
  ],
  "license": "MIT"
}
